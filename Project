<!DOCTYPE html>
<html>
<head>
  <title>Autofill Example</title>
</head>
<body>

<form>
  <label>Name:</label><br>
  <input type="text" id="name" oninput="autoFill()"><br><br>

  <label>Phone Number:</label><br>
  <input type="text" id="phone"><br><br>

  <label>College ID:</label><br>
  <input type="text" id="collegeId"><br><br>

  <label>Address:</label><br>
  <input type="text" id="address"><br><br>
</form>

<script>
  // Stored data
  const studentData = {
    name: "Youssef State",
    phone: "01012345678",
    collegeId: "UFE2025",
    address: "Cairo, Egypt"
  };

  function autoFill() {
    const nameInput = document.getElementById("name").value;

    if (nameInput.toLowerCase() === studentData.name.toLowerCase()) {
      document.getElementById("phone").value = studentData.phone;
      document.getElementById("collegeId").value = studentData.collegeId;
      document.getElementById("address").value = studentData.address;
    }
  }
</script>

</body>
</html>
